    "scripts": {
        "dev": "cross-env STAND=dev NODE_ENV=development webpack-dev-server --config webpack/webpack.dev.js",
        "dev:gost_stage": "cross-env STAND=gost_stage NODE_ENV=development webpack-dev-server --config webpack/webpack.dev.js",
        "dev:ets_test": "cross-env STAND=ets_test NODE_ENV=development webpack-dev-server --config webpack/webpack.dev.js",
        "dev:ets_hotfix": "cross-env STAND=ets_hotfix NODE_ENV=development webpack-dev-server --config webpack/webpack.dev.js",
        "dev:prod": "cross-env STAND=prod NODE_ENV=development webpack-dev-server --config webpack/webpack.dev.js",
        "build": "cross-env STAND=dev NODE_ENV=production webpack --config webpack/webpack.prod.js && node webpack/utils/logVersion.js",
        "build:gost_stage": "cross-env STAND=gost_stage NODE_ENV=production webpack --config webpack/webpack.prod.js && node webpack/utils/logVersion.js",
        "build:ets_test": "cross-env STAND=ets_test NODE_ENV=production webpack --config webpack/webpack.prod.js && node webpack/utils/logVersion.js",
        "build:ets_hotfix": "cross-env STAND=ets_hotfix NODE_ENV=production webpack --config webpack/webpack.prod.js && node webpack/utils/logVersion.js",
        "build:prod": "cross-env STAND=prod NODE_ENV=production webpack --config webpack/webpack.prod.js && node webpack/utils/logVersion.js",
        "lint": "eslint -c .eslintrc.js --fix --ext .js --ext .jsx src",
        "bump": "node webpack/utils/updateVersion.js",
        "prettier": "prettier -c \"src/**/*.tsx\" typescript --parser --write",
        "test": "jest --collectCoverage",
        "lint-staged": "lint-staged",
        "log-version": "node webpack/utils/logVersion.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged && npm run bump && git add package.json"
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx}": [
            "prettier --write",
            "eslint --quiet --fix",
            "git add"
        ],
        "src/**/*.{ts,tsx}": [
            "tslint --project tsconfig.json -c tslint.json --fix",
            "git add"
        ]
    },
    "engines": {
        "node": ">=5.1.0",
        "npm": ">=3.5.2"
    }
