{
  "compilerOptions": {
    "allowJs": true,
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": false,
    "isolatedModules": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noLib": false,
    "noEmit": false,
    "jsx": "react",
    "outDir": "dist",
    "strictFunctionTypes": false,
    "lib": [
      "dom",
      "es2017"
    ],
    "baseUrl": "./src",
    "typeRoots": [
      "node_modules/@types",
      "src/@types"
    ],
    "types": [
      "classnames",
      "jest",
      "lodash",
      "memoize-one",
      "node",
      "ol",
      "prop-types",
      "query-string",
      "ramda",
      "react",
      "react-bootstrap",
      "react-redux",
      "react-router-dom",
      "react-select",
      "react-widgets",
      "recompose",
      "styled-components",
      "webpack-env",
      "typescript",
    ]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "static"
  ],
  "plugins": [
    {
      "name": "typescript-styled-plugin",
      "tags": [
        "styled",
        "css"
      ]
    }
  ]
}
